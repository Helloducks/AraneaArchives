@model IEnumerable<AraneaArchives.Models.Notes>
@{
    
}
<h1>Notes List</h1>
<div class="list-group">

@foreach(var n in Model){
    <p>
            
                <a asp-controller="Read" asp-action="OpenPage" asp-route-id="@n.NotesId" class="list-group-item list-group-item-action ">
        @Html.DisplayFor(modelItem => n.NotesName)
            </a>
            
    </p>
}

</div>